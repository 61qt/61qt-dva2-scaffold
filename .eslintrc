{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "generator-star-spacing": [0],
    "consistent-return": [0],
    "react/forbid-prop-types": [0],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "global-require": [0],
    "import/prefer-default-export": [0],
    "react/jsx-no-bind": [0],
    "react/prop-types": [0],
    "react/prefer-stateless-function": [0],
    "no-else-return": [0],
    "react/jsx-closing-bracket-location": [0],
    "prefer-promise-reject-errors": [0],
    "jsx-a11y/click-events-have-key-events": [0],
    "no-restricted-syntax": [0],
    "no-multi-assign": [0],
    "import/no-extraneous-dependencies": [0],
    "jsx-a11y/no-noninteractive-element-interactions": [0],
    "no-use-before-define": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-nested-ternary": [0],
    "no-mixed-operators": [0],
    "react/no-array-index-key": [0],
    "arrow-body-style": [0],
    "import/extensions": [0],
    "no-bitwise": [0],
    "no-cond-assign": [0],
    "import/no-unresolved": [0],
    "require-yield": [1],
    "object-curly-newline": [0],
    "prefer-destructuring": [0],
    "no-restricted-globals": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "react/jsx-closing-tag-location": [0],
    "import/no-named-as-default-member": [0],
    "yoda": [1, "always"],
    "brace-style": [
      2,
      "stroustrup"
    ],
    "max-len": 0,
  },
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "globals": {
    "React": true,
    "window": true,
    "__DEV__": true,
    "__PROD__": true,
    "QINIU_IMG_CDN_URL": true,
    "APP_CONSTANT": true,
    "Blob": true,
    "XMLHttpRequest": true,
    "FormData": true,
    "FileReader": true,
    "location": true,
    "parseInt": true,
    "debugAdd", true,
    "require": true,
    "localStorage": true,
    "document": true,
    "DEFINE_SENTRY_PROJECT_TOKEN": true,
    "DEFINE_SENTRY_PEOJECT_ID": true,
    "DEFINE_API_URL_BASE": true,
    "DEFINE_PROJECT_TYPE": true,
    "DEFINE_BAIDU_TONGJI_KEY": true,
    "DEFINE_RELEASE_VERSION": true,
    "DEFINE_RELEASE_DATE": true,
    "DEFINE_RELEASE_ENV": true,
    "DEFINE_HOTJAR_KEY": true,
  }
}
